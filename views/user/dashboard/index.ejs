<div class="content">
  <%# Include a user name or greeting %>
  <%- include("partials/header", { user: user }) %>
  <aside class="list-filter">
    <%# Include Compose and Tag Library links %>
    <%- include("partials/navigation", { user: user }) %>
    <%# Include sorting options for journal entries %>
    <%- include("partials/filter-sort", { user: user }) %>
  </aside>
  <main class="col" id="list-entry-all">
  <% if (current_user) { %>
    <% for (let entry of entries) { %>
      <%# Include Journal Entries %>
      <%- include("partials/journal-entry", { user, entry, image: entry.image }) %>
    <% } %>
  <% } else { %>
    <% for (let entry of entries) { %>
      <% if (entry.visible === true) { %>
        <%# Include Journal Entries %>
        <%- include("partials/journal-entry", { user, entry, image: entry.image }) %>
      <% } %>
    <% } %>
  <% } %>
  </main>
</div>