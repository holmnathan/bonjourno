<%# ---- Journal Entry Card with Edit and Delete buttons ---- %>

<section class="card-entry">
  <%# Include Journal Entry Card %>
  <%- include("entry-card", { entry: entry, user: user }) %>
  <nav>
    <form>
    <ul class="button-list form-layout-row">
      <li>
        <button formmethod="GET" type="submit" formaction="/<%= user.username %>/<%= entry.id %>/editor" class="button button-clear button-round button-circle-medium">E</button>
      </li>
      <li>
        <button formmethod="POST" type="submit" formaction="/<%= user.username %>/<%= entry.id %>?_method=DELETE" class="button-icon"><img src="/icons/icon-delete-entry.svg" /></button>
        <address class="author">Jason Gennaro</address>
      </li>
    </ul>
    </form>
  </nav>
</section>