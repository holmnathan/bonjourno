<%# ---- Journal Entry Card with Edit and Delete buttons ---- %>

<section class="card-entry">
  <%# Include Journal Entry Card %>
  <%- include("entry-card", { entry, user, image }) %>
  <nav>
    <form>
    <ul class="button-list form-layout-row">
      <li>
        <button aria-label="Edit Journal Entry"formmethod="GET" type="submit" formaction="/<%= user.username %>/<%= entry.id %>/editor" class="button-icon"><span class="fi fi-rr-edit"></span></button>
      </li>
      <li>
        <button aria-label="Delete Journal Entry" formmethod="POST" type="submit" formaction="/<%= user.username %>/<%= entry.id %>?_method=DELETE" class="button-icon"><span class="fi fi-rr-file-delete"></span></button>
      </li>
    </ul>
    </form>
  </nav>
</section>