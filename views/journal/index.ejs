<div class="content content-row">
<aside id="metadata">
  <ul>
    <% if (entry.location_name) { %><li><%= entry.location_name %></li><% } %>
    <% if (entry.weather_condition) { %><li><%= entry.weather_condition %></li><% } %>
    <% if (entry.weather_temp_f) { %><li><%= entry.weather_temp_f %><sup>ÂºF</sup></li><% } %>
    <% if (entry.date_time) { %><li><%= entry.date_time %></li><% } %>
  </ul>
</aside>
<main class="content-main">
  <% console.log(image) %>
  <% if (typeof image !== "undefined") { %>
    <%- include("../image/partials/image", {image}); %>
  <% } else { %>
    <%- include("../image/partials/image") %>
  <% } %>
  <%- include("partials/navigation", { user: user }) %>
  <h2><%= entry.title %></h2>
  <p><%= entry.body %></p>
</main>
</div>